{
    "name": "my-app",
    "dockerComposeFile": ["../docker-compose.yml"],
    "appPort": [3000],
    "settings": {
      "editor.codeActionsOnSave": {
      "source.fixAll": true
      },
    },
    "service": "my-app",
    "extensions": ["dbaeumer.vscode-eslint", "jpoissonnier.vscode-styled-components"],
    "postCreateCommand": "yarn install",
    "workspaceFolder": "/app",
    "shutdownAction": "stopCompose"
}
